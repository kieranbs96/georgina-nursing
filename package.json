{
  "name": "georgina-nursing-portfolio",
  "description": "Georgina Cross Nursing Portfolio",
  "version": "1.0.0",
  "author": "Kieran Smith",
  "dependencies": {
    "gatsby": "^1.9.215",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "gatsby-plugin-sass": "^1.0.17",
    "gatsby-source-filesystem": "^1.5.24",
    "gatsby-transformer-remark": "^1.7.33",
    "postcss": "^6.0.16",
    "react": "^16.2.0",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "node-sass src/layouts/scss/main.scss src/layouts/main.css -w",
    "compile:sass": "node-sass src/layouts/scss/main.scss src/layouts/main.comp.css",
    "concat:css": "concat -o src/layouts/main.concat.css src/layouts/main.comp.css",
    "prefix:css": "postcss --use autoprefixer -c options.json -o src/layouts/main.prefix.css src/layouts/main.concat.css",
    "compress:css": "node-sass src/layouts/main.prefix.css src/layouts/main.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "devDependencies": {
    "ajv": "^6.2.0",
    "autoprefixer": "^7.2.4",
    "babel-eslint": "^8.2.2",
    "concat": "^1.0.3",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1",
    "prettier": "^1.10.2"
  }
}
